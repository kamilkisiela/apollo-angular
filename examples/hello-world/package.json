{
  "name": "hello-world",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "ng serve --aot --proxy-conf proxy.conf.json --port 4200",
    "server": "nodemon api/index.js --watch api --exec babel-node",
    "build": "ng build --aot --prod",
    "test": "npm run build",
    "schema:download": "apollo-codegen download-schema http://localhost:4300/graphql --output schema.json",
    "schema:generate": "apollo-codegen generate src/graphql/*.graphql --schema schema.json --target typescript --output src/graphql/schema.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/platform-server": "^2.4.9",
    "@angular/router": "^3.4.9",
    "apollo-angular": "file:../../",
    "apollo-client": "^1.0.2",
    "core-js": "^2.4.1",
    "graphql-tag": "^1.3.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "~1.1.1",
    "zone.js": "~0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/compiler-cli": "^2.4.9",
    "@types/graphql": "^0.9.0",
    "apollo-codegen": "^0.10.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "casual": "^1.5.3",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "cors": "^2.8.0",
    "express": "^4.14.0",
    "graphql": "^0.9.0",
    "graphql-server-express": "^0.7.0",
    "graphql-tools": "~0.11.0",
    "nodemon": "^1.11.0",
    "ts-node": "2.0.0",
    "tslint": "^4.2.0",
    "typescript": "~2.2.1"
  }
}
