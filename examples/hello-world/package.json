{
  "name": "hello-world",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "ng serve --aot --proxy-conf proxy.conf.json --port 4200",
    "server": "nodemon api/index.js --watch api --exec babel-node",
    "build": "./node_modules/.bin/ngc -p src/tsconfig.aot.json",
    "test": "ng build --aot --prod",
    "postbuild": "npm run tree-shaking",
    "tree-shaking": "node_modules/.bin/rollup -c rollup.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/platform-server": "^2.3.1",
    "@angular/router": "^3.3.1",
    "angular2-apollo": "file:../../",
    "apollo-client": "~0.7.3",
    "core-js": "^2.4.1",
    "graphql-tag": "^1.1.2",
    "rxjs": "^5.0.1",
    "ts-helpers": "~1.1.1",
    "zone.js": "~0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/isomorphic-fetch": "0.0.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.24",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "casual": "^1.5.3",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^2.2.0",
    "cors": "^2.8.0",
    "express": "^4.14.0",
    "graphql": "~0.8.0",
    "graphql-server-express": "^0.5.0",
    "graphql-tools": "~0.9.0",
    "nodemon": "^1.10.2",
    "rollup": "^0.37.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "ts-node": "1.2.1",
    "tslint": "^4.2.0",
    "typescript": "~2.0.10"
  }
