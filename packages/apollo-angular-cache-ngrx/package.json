{
  "name": "apollo-angular-cache-ngrx",
  "description": "Allows to use @ngrx/store as Apollo Cache",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com> (http://github.com/kamilkisiela/)",
  "license": "MIT",
  "main": "./build/bundle.umd.js",
  "module": "./build/src/index.js",
  "jsnext:main": "./build/src/index.js",
  "typings": "./build/src/index.d.ts",
  "version": "1.0.0-beta.0",
  "repository": {
    "type": "git",
    "url": "apollographql/apollo-angular"
  },
  "scripts": {
    "build": "./node_modules/.bin/ngc -p tsconfig.build.json",
    "bundle": "rollup -c rollup.config.js",
    "test": "npm run test-only",
    "test-only": "jest --config jest.config.js",
    "lint": "tslint -c ../../tslint.json -p tsconfig.json --type-check {src,tests}/*.ts",
    "postbuild": "npm run bundle",
    "deploy": "../../scripts/deploy.sh"
  },
  "peerDependencies": {
    "@angular/core": "^6.0.0-rc.0",
    "@ngrx/store": "^6.0.0-beta.1",
    "rxjs": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular/animations": "6.0.0-rc.3",
    "@angular/common": "6.0.0-rc.3",
    "@angular/compiler": "6.0.0-rc.3",
    "@angular/compiler-cli": "6.0.0-rc.3",
    "@angular/core": "6.0.0-rc.3",
    "@angular/http": "6.0.0-rc.3",
    "@angular/platform-browser": "6.0.0-rc.3",
    "@angular/platform-browser-dynamic": "6.0.0-rc.3",
    "@ngrx/store": "6.0.0-beta.1",
    "@types/graphql": "0.13.0",
    "@types/jest": "22.2.3",
    "@types/zen-observable": "0.5.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.8.0",
    "jest": "21.2.1",
    "jest-zone-patch": "0.0.8",
    "rollup": "0.57.1",
    "rxjs": "^6.0.0-rc.0",
    "ts-jest": "22.4.2",
    "typescript": "2.7.2",
    "zone.js": "0.8.26"
  },
  "dependencies": {
    "apollo-cache": "^1.1.0",
    "apollo-cache-inmemory": "^1.1.5"
