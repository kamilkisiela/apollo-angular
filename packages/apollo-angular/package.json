{
  "name": "apollo-angular",
  "description": "Use your GraphQL data in your Angular app, with the Apollo Client",
  "version": "1.2.0",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com> (http://github.com/kamilkisiela/)",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/bundles/ng.apollo.umd.js",
  "module": "build/fesm5/ng.apollo.js",
  "typings": "build/ng.apollo.d.ts",
  "repository": {
    "type": "git",
    "url": "apollographql/apollo-angular"
  },
  "keywords": [
    "apollo",
    "graphql",
    "angular"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.js",
    "test": "yarn test-only",
    "posttest": "yarn test:testing",
    "test-only": "jest --config jest.config.js",
    "test:testing": "jest --config testing/jest.config.js",
    "prebuild": "rm -rf build/",
    "deploy": "yarn build && npm publish build"
  },
  "peerDependencies": {
    "@angular/core": "^6.1.0",
    "apollo-client": "^2.3.4",
    "apollo-link": "^1.0.0",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.20"
  },
  "devDependencies": {
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/compiler-cli": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@types/graphql": "0.13.4",
    "@types/jest": "23.3.1",
    "apollo-cache-inmemory": "1.2.9",
    "apollo-client": "2.4.1",
    "apollo-link": "1.2.2",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "jest": "23.5.0",
    "jest-zone-patch": "0.0.8",
    "ng-packagr": "4.1.1",
    "rxjs": "6.2.2",
    "ts-jest": "23.1.4",
    "typescript": "2.9.2",
    "zone.js": "0.8.26"
  }
}
